}
.weui-media-box::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    /* border-top: 1px solid var(--weui-FG-3); */
    border-top: 1px solid rgba(0,0,0,.1);
    color: var(--weui-FG-3);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    left: 16px;
}
.weui-panel__bd .weui-cell::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    /* border-top: 1px solid var(--weui-FG-3); */
    border-top: 1px solid rgba(0,0,0,.1);
    color: var(--weui-FG-3);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
    left: 16px;
    z-index: 2;
}
.weui-media-box__info__meta {
    padding-right: 1rem;
}
.weui-media-box__info__meta_extra {
    padding-left: 1rem;
    border-left: 1px solid rgba(0, 0, 0, 0.3);
}

/* preview */
.weui-form-preview__bd {
    /* display: flex; */
    /* flex-direction: row; */
    line-height: 2rem;
    font-size: .9rem;
}
/* 替代子元素左右浮动 */
.weui-form-preview__item {
    flex-shrink: 0;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    /* width: 100%;
    height: 100px; */
}
.weui-form-preview::before {
    top: 0;
    /* border-top: 1px solid var(--weui-FG-3); */
    border-top: 1px solid rgba(0,0,0,.1);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.weui-form-preview::after {
    bottom: 0;
    /* border-bottom: 1px solid var(--weui-FG-3); */
    border-bottom: 1px solid rgba(0,0,0,.1);
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.weui-form-preview::after, .weui-form-preview::before {
    content: " ";
    position: absolute;
    left: 0;
    right: 0;
    height: 1px;
    color: var(--weui-FG-3);
}
.weui-form-preview__ft::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    /* border-top: 1px solid var(--weui-DIALOG-LINE-COLOR); */
    border-top: 1px solid rgba(0,0,0,.1);
    color: var(--weui-DIALOG-LINE-COLOR);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.weui-form-preview__label {
    display: flex;
    float: none;
    flex-shrink: 0;
    width: 4.2rem;
    margin-right: 1rem;
}
.weui-form-preview__value {
    display: flex;
    /* flex: 1; */
    /* width: calc(100vw - 8.2rem); */
}
.weui-form-preview__ft {
    display: flex;
    flex-direction: row;
}

/* 操作反馈 */

/* dialog */
.weui-mask, .weui-mask_transparent {
    position: absolute;
    width: 100vw;
    height: 100vh;
}
.weui-dialog {
    pointer-events: auto;
    background-color: #fff;
    position: absolute;
}
.weui-dialog__ft {
    -webkit-box-orient: horizontal;
    -webkit-flex-direction: row;
    flex-direction: row;
}

/* weui-half-screen */

.weui-half-screen-dialog {
    pointer-events: auto;
    position: absolute;
    width: 100vw;
    background-color: #fff;
    overflow: unset;
}
.weui-half-screen-dialog__bd {
    -webkit-box-flex: 10;
    -webkit-flex: 10 1 auto;
    flex: 10 1 auto;
}
.weui-half-screen-dialog__ft {
    -webkit-box-flex: 0;
    -webkit-flex: 0 0 auto;
    -webkit-flex-direction: row;
    -webkit-flex-wrap: wrap;
    flex: 0 0 auto;
    flex-direction: row;
    flex-wrap: wrap;
}
