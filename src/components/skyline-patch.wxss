.weui-media-box_appmsg {
    flex-direction: row;
}
.weui-media-box__info {
    display: flex;
    flex-direction: row;
}
.weui-media-box__info__meta {
    box-sizing: border-box; 
}
.weui-panel::before {
    top: 0;
    /* border-top: 1px solid var(--weui-FG-3); */
