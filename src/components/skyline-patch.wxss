/* weui-uploader */
.weui-uploader__bd, 
.weui-uploader__files {
    display: flex;
    flex-direction: row; 
}
.weui-uploader__input-box::before {
    width: 2px;
    height: 33.33%;
}
.weui-uploader__input-box::after {
    width: 33.33%;
    height: 2px;
}
.weui-uploader__input-box::after, .weui-uploader__input-box::before {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    background-color: #a3a3a3;
}


/* 基础组件 */

/* weui-grid */
/* 伪元素只支持::，不支持:，所以重写了一波 */
.weui-grids::before {
    right: 0;
    height: 1px;
    /* 还没有效果 */
    /* border-top: 1px solid var(--weui-FG-3); */
    border-top: 1px solid rgba(0,0,0,.1);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.weui-grids::after {
    width: 1px;
    bottom: 0;
    /* border-left: 1px solid var(--weui-FG-3); */
    border-left: 1px solid rgba(0,0,0,.1);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleX(.5);
    transform: scaleX(.5);
}
.weui-grids::after, .weui-grids::before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    color: var(--weui-FG-3);
}
.weui-grid::before {
    top: 0;
    width: 1px;
    /* border-right: 1px solid var(--weui-FG-3); */
    border-right: 1px solid rgba(0,0,0,.1);
    -webkit-transform-origin: 100% 0;
    transform-origin: 100% 0;
    -webkit-transform: scaleX(.5);
    transform: scaleX(.5);
}
.weui-grid::after {
    left: 0;
    height: 1px;
    /* border-bottom: 1px solid var(--weui-FG-3); */
    border-bottom: 1px solid rgba(0,0,0,.1);
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(.5);
    transform: scaleY(.5);
}
.weui-grid::after, .weui-grid::before {
    content: " ";
    position: absolute;
    right: 0;
    bottom: 0;
    color: var(--weui-FG-3);
}

.weui-grids {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}
.weui-grid {
    display: flex;
    align-items: center;
}

/* weui-badge */
.weui-badge {
    min-width: 8px;
    padding: 1.8px 4.8px;
}
.weui-badge_dot {
    padding: 4.8px;
    flex: none;
    height: 0;
    width: 0;
}

/* weui-footer */
.weui-footer__links {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
}

/* weui-loadmore */
.weui-loading {
    display: flex;
    height: 20px;
    width: 20px;
}
.weui-loadmore {
    flex-direction: row;
    justify-content: center;
    align-items: center;
}
.weui-loadmore__tips {
    display: flex;
    color: rgba(0, 0, 0, 0.9);
}
.weui-loadmore_line {
    /* border-top: 1px solid var(--weui-FG-3); */
    border-top: 1px solid rgba(0, 0, 0, 0.1);
}
.weui-loadmore_line .weui-loadmore__tips {
    top: -0.9rem;
}
.weui-loadmore_line .weui-loadmore__tips_in-dot{
    top: -2px; 
}
.weui-loadmore_dot .weui-loadmore__tips::before {
    content: " ";
    width: 4px;
    height: 4px;
    border-radius: 50%;
    background-color: var(--weui-FG-3);
    /* background-color: #000; */
    display: inline-block;
    position: relative;
    vertical-align: 0;
    /* top: -.16rem; */
    border-top-left-radius: 50%;
    border-top-right-radius: 50%;
    border-bottom-right-radius: 50%;
    border-bottom-left-radius: 50%;
}

